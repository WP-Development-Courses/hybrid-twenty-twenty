{
  "version": 1,
  "settings": {
    "layout": {
      "contentSize": "610px",
      "wideSize": "1200px"
    },
    "custom" : {
      "colors": {
        "black": "#000000",
        "red": "#cd2653",
        "beige": "#dcd7ca",
        "light-beige": "#f5efe0",
        "grey": "#6d6d6d"
      }
    },
    "color": {
      "palette": [
        {
          "slug": "accent",
          "name": "Accent Color",
          "color": "var(--wp--custom--colors--red)"
        },
        {
          "slug": "primary",
          "name": "Primary",
          "color": "var(--wp--custom--colors--black)"
        },
        {
          "slug": "secondary",
          "name": "Secondary",
          "color": "var(--wp--custom--colors--grey)"
        },
        {
          "slug": "subtle-background",
          "name": "Subtle Background",
          "color": "var(--wp--custom--colors--beige)"
        },
        {
          "slug": "background",
          "name": "Background Color",
          "color": "var(--wp--custom--colors--light-beige)"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--custom--colors--light-beige)",
      "text": "var(--wp--custom--colors--light-black)"
    },
    "typography": {
      "fontSize": "1.8rem"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--custom--colors--red)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "3.6rem"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "3.2rem"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "2.8rem"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "2.4rem"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "2.1rem"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "1.8rem"
        }
      }
    }
  }
}
